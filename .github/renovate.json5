{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "docker:enableMajor",
    "replacements:k8s-registry-move",
    ":automergeBranch",
    ":disableRateLimiting",
    ":dependencyDashboard",
    ":semanticCommits",
    ":skipStatusChecks",
    ":timezone(Europe/London)",
    "github>zariel/holywoo//.github/renovate/allowedVersions.json5",
    "github>zariel/holywoo//.github/renovate/autoMerge.json5",
    "github>zariel/holywoo//.github/renovate/commitMessage.json5",
    "github>zariel/holywoo//.github/renovate/customManagers.json5",
    "github>zariel/holywoo//.github/renovate/grafanaDashboards.json5",
    "github>zariel/holywoo//.github/renovate/groups.json5",
    "github>zariel/holywoo//.github/renovate/packageRules.json5",
    "github>zariel/holywoo//.github/renovate/semanticCommits.json5"
  ],
  "onboarding": false,
  "requireConfig": "optional",
  "dependencyDashboardTitle": "Renovate Dashboard ðŸ¤–",
  "suppressNotifications": [
    "prEditedNotification",
    "prIgnoreNotification"
  ],
  "ignorePaths": [
    "**/*.sops.*",
    "**/.archive/**",
    "**/resources/**"
  ],
  "flux": {
    "fileMatch": [
      "(^|/)k8s/.+\\.ya?ml(\\.j2)?$"
    ]
  },
  "helm-values": {
    "fileMatch": [
      "(^|/)k8s/.+\\.ya?ml(\\.j2)?$"
    ]
  },
  "kubernetes": {
    "fileMatch": [
      "(^|/)\\.taskfiles/.+\\.ya?ml(\\.j2)?$",
      "(^|/)k8s/.+\\.ya?ml(\\.j2)?$"
    ]
  }
}
