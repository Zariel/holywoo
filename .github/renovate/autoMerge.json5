{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "ignoreTests": true,
  "packageRules": [
    {
      "description": [
        "Auto-merge container updates for trusted containers"
      ],
      "matchDatasources": [
        "docker"
      ],
      "automerge": true,
      "automergeType": "branch",
      "matchPackagePatterns": [
        "ghcr.io/bjw-s",
        "ghcr.io/onedr0p"
      ]
    }
  ]
}
