{
  "repositories": [
    "zariel/holywoo"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "matchCurrentVersion": "!/^0/",
      "automerge": true
    }
  ],
  "ignoreTests": true,
  "kubernetes": {
    "fileMatch": [
      "\\.yaml$"
    ]
  }
}
